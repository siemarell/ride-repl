{
  "compilerOptions": {
    "baseUrl": "src",
    "moduleResolution": "node",
    "outDir": "./dist/",
    "sourceMap": false,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "paths": {
      "@utils/*": [
        "utils/*"
      ]
    }
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "web",
    "node_modules",
    "dist"
  ]
}